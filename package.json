    "scripts": {
        "build": "NODE_ENV=production node devtools/commands/build.js",
        "dev": "node devtools/commands/dev.js",
        "generate:schema": "graphql-codegen",
        "generate:style": "node devtools/commands/extractTheme.js",
        "lint": "eslint . --ext .js,.ts,.tsx,.graphql",
        "lint:fix": "yarn lint --fix",
        "tsc": "tsc",
        "test": "NODE_ENV=test jest --runInBand",
        "test:coverage": "yarn test --coverage",
        "cypress:open": "NODE_ENV=test yarn run cypress open",
        "cypress:run": "NODE_ENV=test yarn run cypress run",
        "prepare": "husky install"
    },
